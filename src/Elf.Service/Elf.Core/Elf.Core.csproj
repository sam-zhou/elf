    <Content Include="Emailing\EmailTemplates\*.html" CopyToPublishDirectory="Always" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Authentication\MyAXA\Models\**" />
    <EmbeddedResource Remove="Authentication\MyAXA\Models\**" />
