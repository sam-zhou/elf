    <RootNamespace>Elf.Core</RootNamespace>
    <SkipValidatePackageReferences>true</SkipValidatePackageReferences>
    <AssemblyName>Elf.Core</AssemblyName>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="Localization\**\*.xml" />
